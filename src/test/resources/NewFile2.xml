<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="annotation">

	<parameter name="browsername" value="edge" />
	<parameter name="url" value="http://www.google.com/" />
	<test name="annotation test">
		<classes>
			<class name="org.annotations.parameters">

				<methods>

					<include name="launchGoogle" />
					<include name="verifyGoogleTitle" />

				</methods>
			</class>
		</classes>
	</test>
</suite>